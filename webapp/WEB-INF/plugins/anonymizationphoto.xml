<?xml version="1.0" encoding="UTF-8" standalone="no"?><plug-in>
	<name>anonymizationphoto</name>
	<class>fr.paris.lutece.portal.service.plugin.PluginDefaultImplementation
	</class>
	<version>1.0.0</version>
	<documentation/>
	<installation/>
	<changes/>
	<user-guide/>
	<description>anonymizationphoto.plugin.description</description>
	<provider>anonymizationphoto.plugin.provider</provider>
	<provider-url>http://lutece.paris.fr</provider-url>
	<icon-url>images/admin/skin/feature_default_icon.png</icon-url>
	<copyright>Copyright (c) 2020 Mairie de Paris</copyright>
	<db-pool-required>0</db-pool-required>

	<core-version-dependency>
		<min-core-version>6.0.1</min-core-version>
		<max-core-version/>
	</core-version-dependency>

	<css-stylesheets>
		<css-stylesheets-scope>portal</css-stylesheets-scope>
		<css-stylesheet>forms/forms.css</css-stylesheet>
	</css-stylesheets>

	<admin-css-stylesheets>
		<admin-css-stylesheet>css/admin/plugins/forms/forms.css</admin-css-stylesheet>
	</admin-css-stylesheets>

    <daemons>
	 <daemon>
	  <daemon-id>anonymizationphotoDaemon</daemon-id>
	  <daemon-name>anonymizationphoto.daemon.anonymizationphotodaemon.name</daemon-name>
	  <daemon-description>anonymizationphoto.daemon.anonymizationphotodaemon.description</daemon-description>
	  <daemon-class>fr.paris.lutece.plugins.anonymizationphoto.service.daemon.AnonymizationPhoto</daemon-class>
	 </daemon>
	</daemons>
	
	<!--Administration interface parameters -->
	<admin-features>
		<admin-feature>
			<feature-id>STAT_ANONYMIZATION_PHOTO</feature-id>
			<feature-title>anonymizationphoto.adminFeature.stat.anonymizationphoto.name</feature-title>
			<feature-description>archivagedansmarue.adminFeature.stat.anonymizationphoto.description</feature-description>
			<feature-level>3</feature-level>
			<feature-url>jsp/admin/plugins/anonymizationphoto/StatAnonymizationPhoto.jsp</feature-url>
		</admin-feature>
	</admin-features>

	<!-- XPages parameters -->
	<applications>
